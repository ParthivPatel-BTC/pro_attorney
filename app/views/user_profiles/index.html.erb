<div class="box box-primary">
	<div class="box-header with-border">
		<h3 class="box-title custom-header">Advocates</h3>
	</div>
<div class="box-body">
	<table class="table table-striped table-bordered table-hover">
		<tr><th>Name</th></tr>
		<% @user_profile.each do |user_profile|  %>
		<tr>
			<td><% if user_profile.avatar.exists? %>
                <%= image_tag(user_profile.avatar.url,height: "50px", width: "50px",id: "output" ,class: "img-circle") %>
                <% else %> 
                <%= image_tag('default.jpg', height: "50px", width: "50px",id: "output",class: "img-circle") %>
                <% end %> 
                <%=link_to user_profile_path(user_profile) do%>
				<%= user_profile.first_name.titleize + " " + user_profile.last_name.titleize %>
				<div class= "pull-right" >
				<%= rating_for user_profile, "ratings", disabled: true, imdb_avg: true%>
			</div>
			<%end %></td>
			<% if user_profile.user.id==current_user.id %>
				<td>
					<%= link_to edit_user_profile_path(user_profile), class: "fa fa-fw fa-pencil-square-o", title: "Edit" do %><% end %>
				</td>
				<td>
					<%= link_to "", method: :delete, data: {confirm: 'are you sure? '}, class: "fa fa-fw fa-trash", title: "delete" do %><% end %>
				</td>
			<% end %>
		</tr>
		<%end%>
	</table>
</div>
</div>
	<%= will_paginate @user_profile, renderer: BootstrapPagination::Rails, class: "pull-right" %>

