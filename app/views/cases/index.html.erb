<h1>Cases</h1>
<%= render partial: "search_form" %>
  <% if @cases.blank? %>
    <%= flash[:notice] %>
  <% else %>
  <table class="table table-striped">
  <thead>
    <tr>
      <th>Title</th>
      <th>Location</th>
      <th>Status</th>
      <th colspan="1"></th>
    </tr>
  </thead>
  <tbody>
    <%= render partial: "cases" %>
  </tbody>
</table>
<%= link_to 'New Case', new_case_path %>