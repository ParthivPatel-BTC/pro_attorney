<h1>User Profiles</h1>
<div align="right">
	<%= link_to edit_user_profile_path(@user_profile), class: "btn btn-primary" do %>
			Register Today
			<%end%>
		</div>
	
<table class="table table-striped table-hover">
	<tr>
		 <th>First name</th> <th>Last Name</th> <th>Actions</th>
	</tr>
	<% @user_profile.each do |up|  %>
	<tr>
		
		<td><%= up.first_name %></td>
		<td><%= up.last_name %></td>
		<td>
			<%= link_to "Show", user_profile_path(up) %>
			<%= link_to "Edit", edit_user_profile_path(up) %>
			<%= link_to "Delete", up, method: :delete, data: {confirm: 'are you sure? '} %>
		</td>
	</tr>
	<% end %>
</table>
