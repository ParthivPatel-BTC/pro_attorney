<%= form_for(@case)  do |f| %>
<table>
	<tr>

<td><%=f.label :Client_id %></td>
<td><%=f.text_field :client_id %></td>

</tr>

<tr>

<td><%=f.label :Advocate_id %></td>

<td><%=f.text_field :advocate_id %></td>


</tr>
<tr>
	<td><%=f.label :Case_Type %></td>
	<td><%= f.select :case_type_id, options_from_collection_for_select(CaseType.all, :id, :case_type) %></td>

</tr>




<tr>

<td><%=f.label :Case_Title %></td>
<td><%=f.text_field :case_title %></td>


</tr>



<tr>
<td><%=f.label :Case_detail %> </td>
<td><%=f.text_area :case_detail %></td>
</tr>



<tr>
<td><%=f.label :Location %></td>
<td><%=f.text_field :location %></td>
</tr>


<tr>
	<td>
<%=f.label :Status %></td>
<td><%=f.text_field :status %></td>
</tr>

<tr>

<td><%=f.submit %></td>






<%end%>
